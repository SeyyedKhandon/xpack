{
  "name": "zpack",
  "version": "1.0.6",
  "publisher": "SeyyedKhandon",
  "author": "SeyyedMahdi Hassanpour",
  "displayName": "Web Essentials Extension Pack (ZPack series)",
  "description": "An Opinionated collection/pack of essentials extensions for Web Developers in VSCode (Better DX in Mind)",
  "license": "MIT",
  "galleryBanner": {
    "color": "#47B784",
    "theme": "dark"
  },
  "icon": "zpack.jpg",
  "repository": {
    "url": "https://github.com/SeyyedKhandon/zpack.git"
  },
  "homepage": "https://github.com/SeyyedKhandon/zpack.git",
  "bugs": {
    "url": "https://github.com/SeyyedKhandon/zpack.git/issues"
  },
  "keywords": [
    "web",
    "zpack",
    "developer experience",
    "web dx extension pack",
    "extension pack",
    "DX Enhancer Pack (EPack)",
    "Frontend Extension Pack (FPack)",
    "Git Extension Pack (GPack)",
    "Web Quality/Metric Extension Pack (QPack)",
    "auto close tag, better comments,... ",
    "One Dark Pro",
    "Material Icon Pack",
    "Fira Code font",
    "ESlint",
    "Code Metrics",
    "Code Spell Checker",
    "Error Lens",
    "Import Cost",
    "git Extension Pack",
    "conventional commit",
    "extension pack",
    "git file history",
    "git graph",
    "git lens",
    "HTMLHint",
    "HTML CSS Support",
    "Auto Rename Tag",
    "Color Highlight",
    "Color Info",
    "Color Manager",
    "CSS Navigation",
    "Image preview",
    "Live Server",
    "Static server",
    "file-size",
    "Project Manager",
    "Better Comments",
    "Bracket Pair Colorizer",
    "Markdown Preview Enhanced",
    "Prettier - Code formatter",
    "Formatting Toggle"
  ],
  "categories": [
    "Extension Packs"
  ],
  "engines": {
    "vscode": "^1.55.0"
  },
  "activationEvents": [
    "onCommand:zpack.updateConfig"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "zpack.updateConfig",
        "title": "Update Essentials Web Extension Pack (ZPack series) Config"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "extensionPack": [
    "SeyyedKhandon.gpack",
    "SeyyedKhandon.qpack",
    "SeyyedKhandon.epack",
    "SeyyedKhandon.fpack",
    "SeyyedKhandon.tpack"
  ],
  "devDependencies": {
    "@types/vscode": "^1.55.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.19.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
